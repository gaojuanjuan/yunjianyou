<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.yunjy.jianyou">




    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>







    <application
        android:allowBackup="true"
        android:icon="@mipmap/shirt"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/shirt"
        android:supportsRtl="true"
        android:theme="@style/AppTheme_jianyou">
        <activity
            android:name=".page.act.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".page.act.CommodityDetailAct"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".page.act.FeedbackAct"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".page.act.CollectionAct"
                  android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>